--- gem5/benchmarks/sys_validation/mergesort/sw/boot.s	1969-12-31 18:00:00.000000000 -0600
+++ gem5-SALAM/benchmarks/sys_validation/mergesort/sw/boot.s	2024-02-22 18:17:14.840943117 -0600
@@ -0,0 +1,178 @@
+/*
