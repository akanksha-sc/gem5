+    if (dbg) DPRINTFS(RuntimeCompute, owner, "|| %s = %s\n", ir_stub, resstr.c_str());
+    setRegisterValue(result);
+#else
+    uint64_t bitcastResult;
+    switch(size) {
+        case 32:
+        {
+            float op1 = operands.at(0).getFloatFromReg();
+            float op2 = operands.at(1).getFloatFromReg();
