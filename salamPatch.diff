+Port &
+StreamBuffer::getPort(const std::string &if_name, PortID idx)
+{
+    if (if_name == "stream_in") {
+        return streamIn;
+    } else if (if_name == "stream_out") {
+        return streamOut;
+    } else if (if_name == "status_in") {
+        return statusIn;
+    } else if (if_name == "status_out") {
+        return statusOut;
+    }
+    return ClockedObject::getPort(if_name, idx);
+}
