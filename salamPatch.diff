+std::string
+SALAM::APFloatRegister::dataString() {
+    std::stringstream ss;
+#if USE_LLVM_AP_VALUES
+
+#else
+    float fdata = *(float*)&data;
+    double ddata = *(double*)&data;
+    ss << fdata << "f " << ddata << "d";
+#endif
+    return ss.str();
+}
+
+std::string
+SALAM::APIntRegister::dataString() {
+    std::stringstream ss;
+#if USE_LLVM_AP_VALUES
+
+#else
+    ss << "0x" << std::hex << data;
+#endif
+    return ss.str();
